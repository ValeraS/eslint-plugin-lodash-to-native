{
  "name": "eslint-plugin-lodash-to-native",
  "version": "0.0.1",
  "description": "Converts lodash map to array map",
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin"
  ],
  "author": "Valeriy Sidorenko",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "format": "eslint . --fix",
    "test": "mocha tests --recursive"
  },
  "dependencies": {
    "requireindex": "~1.1.0"
  },
  "devDependencies": {
    "eslint": "~4.18.2",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.0.5",
    "lint-staged": "^9.3.0",
    "mocha": "^3.1.2",
    "prettier": "^1.18.2"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "license": "MIT"
}
